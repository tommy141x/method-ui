{
  "name": "method",
  "version": "0.1.0",
  "description": "Method UI Component Library CLI - Add beautiful Solid.js components to your project",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "method": "./dist/cli.js"
  },
  "files": [
    "dist/",
    "lib/",
    "components/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "build": "bun run scripts/build.ts",
    "cli": "bun run cli/cli.ts",
    "compile": "bun run scripts/compile.ts",
    "watch": "bun run scripts/watch.ts",
    "update": "bun run scripts/update.ts",
    "check:licenses": "license-checker scan --failOn GPL-2.0 GPL-3.0 LGPL-2.1 LGPL-3.0 AGPL-3.0 MPL-2.0 EPL-1.0 EPL-2.0 CDDL-1.0",
    "prepublishOnly": "bun run build && bun run check:licenses",
    "test:publish": "bun publish --dry-run"
  },
  "keywords": [
    "method",
    "method-ui",
    "solidjs",
    "solid",
    "components",
    "ui",
    "cli",
    "component-library",
    "design-system",
    "ark-ui"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/method.git"
  },
  "homepage": "https://github.com/yourusername/method#readme",
  "bugs": {
    "url": "https://github.com/yourusername/method/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@unocss/reset": "^66.5.6",
    "citty": "^0.1.6",
    "echarts": "^6.0.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@ark-ui/solid": "^5.27.1",
    "@iconify-json/lucide": "^1.2.73",
    "@onebeyond/license-checker": "^2.1.0",
    "@responsive-image/solid": "^2.0.4",
    "@tiptap/core": "^3.10.7",
    "@tiptap/pm": "^3.10.7",
    "@types/bun": "^1.3.2",
    "@unocss/preset-icons": "^66.5.6",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "rough-notation": "^0.5.1",
    "solid-js": "^1.9.10",
    "solid-motionone": "^1.0.4",
    "solid-tiptap": "^0.8.0",
    "unocss": "^66.5.6",
    "unocss-merge": "^0.6.0"
  }
}
